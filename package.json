{
  "name": "back",
  "version": "1.0.0",
  "description": "``` npm install ```",
  "main": "index.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "tsc": "tsc",
    "dev": "ts-node-dev src/index.ts ",
    "start:dev": "ts-node-dev --respawn --transpile-only --exit-child src/index.ts ",
    "start:test:server": "ts-node-dev --respawn --transpile-only --exit-child src/index.ts ",
    "lint": "eslint --ext .ts .",
    "start": "node build/src/index.js",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "initdb": "npx prisma migrate dev --name init && npx prisma db seed",
    "test": "jest",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:generate": "npx prisma generate",
    "build": "tsc"
  },
 
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "prisma": "^5.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.3",
    "uuid": "^9.0.0"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21"
  }
}
