{
  "compilerOptions": {
    "target": "ES6",
    "outDir": "./build/",
    "rootDir": "./",
    "module": "commonjs",
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "exclude": ["__tests__", "data"]
}
